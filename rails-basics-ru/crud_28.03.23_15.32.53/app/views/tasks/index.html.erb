<h1>Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th>Creator</th>
      <th>Performer</th>
      <th>Completed</th>
    </tr>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.name %></td>
        <td><%= task.description %></td>
        <td><%= task.status %></td>
        <td><%= task.creator %></td>
        <td><%= task.performer %></td>
        <td><%= task.completed %></td>
      </tr>
    <% end %>
  </tbody>
</table>